{
    "name": "backend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:deploy:railway": "bash scripts/railway-predeploy.sh",
        "lint": "eslint src/",
        "check:runtime-config": "tsx src/scripts/check-runtime-config.ts",
        "test:camoufox": "tsx src/scripts/test-camoufox-integration.ts",
        "delete-evolving-ai": "tsx -r dotenv/config scripts/delete-evolving-ai-data.ts",
        "backfill-brain-profiles": "tsx -r dotenv/config scripts/backfill-brain-profiles.ts",
        "baseline:current-users": "tsx -r dotenv/config scripts/export-current-user-baseline.ts",
        "audit:db-architecture": "tsx -r dotenv/config scripts/audit-db-architecture.ts",
        "cleanup:out-of-scope-media-analyses": "tsx -r dotenv/config scripts/cleanup-out-of-scope-media-analyses.ts",
        "backfill:scoped-media-analyses": "tsx -r dotenv/config scripts/backfill-scoped-media-analyses.ts",
        "remediate:legacy-jobs": "tsx -r dotenv/config scripts/remediate-legacy-jobs.ts",
        "test:workflow-integrity": "tsx -r dotenv/config scripts/test-client-workflow-integrity.ts"
    },
    "dependencies": {
        "@prisma/client": "^6.0.0",
        "@types/fluent-ffmpeg": "^2.1.28",
        "@types/node-cron": "^3.0.11",
        "axios": "^1.13.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "ffmpeg-static": "^5.3.0",
        "fluent-ffmpeg": "^2.1.3",
        "multer": "^1.4.5-lts.1",
        "node-cron": "^4.2.1",
        "openai": "^4.104.0",
        "puppeteer": "^24.36.1",
        "puppeteer-extra": "^3.3.6",
        "puppeteer-extra-plugin-stealth": "^2.11.2",
        "python-shell": "^5.0.0",
        "sharp": "^0.34.5",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.10.0",
        "@types/python-shell": "^0.4.2",
        "@types/uuid": "^9.0.7",
        "prisma": "^6.0.0",
        "tsx": "^4.6.0",
        "typescript": "^5.3.0"
    }
